{
  "expo": {
    "name": "Artizen",
    "slug": "artizen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_artizen.png",
    "scheme": "artizen",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logo_artizen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_artizen.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logo_artizen.png",
      "name": "Artizen"
    }
  }
}